# Optional API key for client authentication
# If not set, server will prompt for interactive API key protection on startup
# API_KEY=your-optional-api-key

# Server port
PORT=8000

# Timeout in milliseconds
MAX_TIMEOUT=600000

# CORS origins (JSON array)
CORS_ORIGINS=["*"]

# Debug mode
DEBUG_MODE=false

# Verbose logging
VERBOSE=false
